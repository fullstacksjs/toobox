{
  "extends": "@fullstacksjs/tsconfig/cjs",
  "compilerOptions": {
    "outDir": "./lib/cjs",
    "declaration": false,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    "allowJs": true,
    "types": ["./lib.deno.d.ts", "node"]
  },
  "include": ["src/"],
  "exclude": ["**/*.spec.ts"]
}
